{
  "name": "use-rx-hooks",
  "version": "1.6.0",
  "description": "Collection of React hooks with RxJS",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "lib/",
    "esm/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "yarn storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn build:cjs && yarn build:es",
    "clean": "rimraf lib storybook-static esm",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:upload": "gh-pages -d storybook-static --git \"$(which git)\"",
    "storybook:clean": "rimraf storybook-static",
    "prepublishOnly": "yarn test && yarn build && yarn storybook:build && yarn storybook:upload"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:types && lint-staged"
    }
  },
  "author": "@Pong420",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "rxjs": ">=6.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@storybook/addon-centered": "5.2.3",
    "@storybook/addon-docs": "5.2.3",
    "@storybook/addon-links": "5.2.3",
    "@storybook/addons": "5.2.3",
    "@storybook/react": "5.2.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.9",
    "@types/react": "16.9.44",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.8.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "4.5.2",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "fork-ts-checker-webpack-plugin": "5.0.6",
    "gh-pages": "^3.1.0",
    "husky": "4.2.5",
    "jest": "26.2.2",
    "lint-staged": "10.2.11",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "3.0.2",
    "rxjs": "6.6.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "ts-jest": "^26.1.4",
    "ts-loader": "8.0.2",
    "ts-node": "8.10.2",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --rule 'no-console: [\"error\", { allow: [\"warn\", \"error\"] }]' --max-warnings=0",
      "prettier --ignore-path .eslintignore --write"
    ]
  }
}
