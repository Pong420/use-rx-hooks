{
  "name": "use-rx-hooks",
  "version": "1.0.2",
  "description": "Collection of React hooks with RxJS",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "lib/",
    "esm/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "yarn storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint 'src/**/*.{ts,tsx}' -t verbose",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn build:cjs && yarn build:es",
    "clean": "rimraf lib storybook-static esm",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:upload": "gh-pages -d storybook-static --git \"$(which git)\"",
    "storybook:clean": "rimraf storybook-static"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:types && lint-staged",
      "pre-publish": "yarn build"
    }
  },
  "author": "@Pong420",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@storybook/addon-centered": "^5.2.0-rc.11",
    "@storybook/addon-docs": "^5.2.0-rc.11",
    "@storybook/addon-links": "^5.2.0-rc.11",
    "@storybook/addons": "^5.2.0-rc.11",
    "@storybook/react": "^5.2.0-rc.11",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "babel-loader": "^8.0.6",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "gh-pages": "^2.1.1",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "prettier": "^1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "ts-loader": "6.1.0",
    "ts-node": "8.3.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.6.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix -t verbose",
      "git add"
    ]
  }
}
