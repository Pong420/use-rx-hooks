{
  "name": "use-rx-hooks",
  "version": "1.0.2",
  "description": "Collection of React hooks with RxJS",
  "main": "lib/index.js",
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "yarn storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions '.ts,.tsx' --ignore 'src/__stories__'",
    "build": "yarn build:types && yarn build:js",
    "clean": "rimraf lib storybook-static esm",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:upload": "gh-pages -d storybook-static --git \"$(which git)\"",
    "storybook:clean": "rimraf storybook-static",
    "prettier": "prettier"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:types && lint-staged",
      "pre-publish": "yarn build"
    }
  },
  "author": "@Pong420",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-centered": "^5.2.0-rc.11",
    "@storybook/addon-docs": "^5.2.0-rc.11",
    "@storybook/addon-links": "^5.2.0-rc.11",
    "@storybook/addons": "^5.2.0-rc.11",
    "@storybook/react": "^5.2.0-rc.11",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.2.0",
    "@typescript-eslint/parser": "2.2.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.1",
    "eslint": "6.4.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "gh-pages": "^2.1.1",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "prettier": "^1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "ts-loader": "6.1.0",
    "ts-node": "8.3.0",
    "typescript": "^3.6.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
